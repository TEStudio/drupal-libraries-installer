{
    "name": "balbuf/drupal-libraries-installer",
    "description": "Install Drupal libraries via a simple listing in your composer.json file",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephen Beemsterboer",
            "homepage": "https://github.com/balbuf"
        },
        {
            "name": "Vladimir Roudakov",
            "homepage": "https://github.com/balbuf"
        }
    ],
    "support": {
        "issues": "https://github.com/testudio/drupal-libraries-installer/issues"
    },
    "autoload": {
        "psr-4": {"DrupalLibrariesInstaller\\": "src/"}
    },
    "extra": {
        "class": "DrupalLibrariesInstaller\\Plugin"
    },
    "require": {
        "composer-plugin-api": "^1.0 | ^2.0",
        "composer/installers": "^1.1"
    }
}
